+		}
+		if p.NoUserGroup {
+			addErr(errors.ErrPasswdCreateAndNoUserGroup)
+		}
+		if p.PrimaryGroup != "" {
+			addErr(errors.ErrPasswdCreateAndPrimaryGroup)
+		}
+		if p.Shell != "" {
+			addErr(errors.ErrPasswdCreateAndShell)
+		}
+		if p.System {
+			addErr(errors.ErrPasswdCreateAndSystem)
+		}
+		if p.UID != nil {
+			addErr(errors.ErrPasswdCreateAndUID)
+		}
+	}
+	return r

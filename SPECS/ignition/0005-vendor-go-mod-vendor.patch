+		return res
+	}
+	translateSystemdDropinSlice := func(old []from.SystemdDropin) []types.SystemdDropin {
+		var res []types.SystemdDropin
+		for _, x := range old {
+			res = append(res, types.SystemdDropin{
+				Contents: x.Contents,
+				Name:     x.Name,
+			})

+				Start:    int(oldPartition.Start),
+				TypeGUID: string(oldPartition.TypeGUID),
+			})
+		}
+
+		config.Storage.Disks = append(config.Storage.Disks, disk)
+	}
+
+	for _, oldArray := range old.Storage.Arrays {

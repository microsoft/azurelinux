 grub_TRANSFORM([grub-mkfont.3])
@@ -105,6 +106,7 @@ grub_TRANSFORM([grub-render-label.3])
 grub_TRANSFORM([grub-script-check.3])
 grub_TRANSFORM([grub-set-default.1])

[azurelinux-official-debug-preview]
name=Azure Linux Official Debuginfo Preview $releasever $basearch
baseurl=https://packages.microsoft.com/azurelinux/$releasever/preview/base/debuginfo/$basearch
gpgkey=file:///etc/pki/rpm-gpg/MICROSOFT-RPM-GPG-KEY
gpgcheck=1
repo_gpgcheck=1
enabled=1
skip_if_unavailable=True
sslverify=1

[azurelinux-official-extended-debug]
name=Azure Linux Official Extended DebugInfo $releasever $basearch
baseurl=https://packages.microsoft.com/azurelinux/$releasever/prod/extended/debuginfo/$basearch
gpgkey=file:///etc/pki/rpm-gpg/MICROSOFT-RPM-GPG-KEY
gpgcheck=1
repo_gpgcheck=1
enabled=1
skip_if_unavailable=True
sslverify=1

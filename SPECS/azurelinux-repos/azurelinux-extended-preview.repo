[azurelinux-official-extended-preview]
name=Azure Linux Official Extended Preview $releasever $basearch
baseurl=https://packages.microsoft.com/azurelinux/$releasever/preview/extended/$basearch
gpgkey=file:///etc/pki/rpm-gpg/MICROSOFT-RPM-GPG-KEY
gpgcheck=1
repo_gpgcheck=1
enabled=1
skip_if_unavailable=True
sslverify=1

[azurelinux-official-extended-preview-debuginfo]
name=Azure Linux Official Extended Preview $releasever $basearch Debuginfo
baseurl=https://packages.microsoft.com/azurelinux/$releasever/preview/extended/debuginfo/$basearch
gpgkey=file:///etc/pki/rpm-gpg/MICROSOFT-RPM-GPG-KEY
gpgcheck=1
repo_gpgcheck=1
enabled=0
skip_if_unavailable=True
sslverify=1

[azurelinux-official-extended-preview-source]
name=Azure Linux Official Extended Preview $releasever Source
baseurl=https://packages.microsoft.com/azurelinux/$releasever/preview/extended/srpms
gpgkey=file:///etc/pki/rpm-gpg/MICROSOFT-RPM-GPG-KEY
gpgcheck=1
repo_gpgcheck=1
enabled=0
skip_if_unavailable=True
sslverify=1

 .SH JMÉNO
-hypot \- eukleidovská vzdálenost
+hypot, hypotf, hypotl \- eukleidovská vzdálenost
 .SH SYNTAXE
 .nf
 .B #include <math.h>
 .sp
 .BI "double hypot(double " x ", double " y );
+.BI "float hypotf(float " x ", float " y );
+.BI "long double hypotl(long double " x ",long  double " y );
 .fi
